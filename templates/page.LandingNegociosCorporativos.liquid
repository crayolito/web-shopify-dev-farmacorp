    // Función para crear la solicitud SOAP
    function createSOAPRequest(formData, token) {
        console.log('Creando solicitud SOAP con los siguientes datos:', formData, 'y token:', token);
        return `
            <soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:web="http://soap.sforce.com/schemas/class/WSNegociosCorporativos">
                <soapenv:Header>
                    <web:SessionHeader>
                        <web:sessionId>${token}</web:sessionId>
                    </web:SessionHeader>
                </soapenv:Header>
                <soapenv:Body>
                    <web:recibirNegociosCorporativos>
                        <web:negocios>
                            <web:nombresApellidos>${formData.nombresApellidos}</web:nombresApellidos>
                            <web:ciudadRegional>${formData.ciudadRegional}</web:ciudadRegional>
                            <web:correoElectronico>${formData.correoElectronico}</web:correoElectronico>
                            <web:datosInteres>${formData.datosInteres}</web:datosInteres>
                            <web:mensaje>${formData.mensaje}</web:mensaje>
                            <web:nitEmpresa>${formData.nitEmpresa}</web:nitEmpresa>
                            <web:nombreEmpresa>${formData.nombreEmpresa}</web:nombreEmpresa>
                            <web:telefonoCelular>${formData.telefonoCelular}</web:telefonoCelular>
                        </web:negocios>
                    </web:recibirNegociosCorporativos>
                </soapenv:Body>
            </soapenv:Envelope>
        `;
    }

