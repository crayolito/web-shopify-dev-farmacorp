    // Evento que se ejecuta cuando el DOM ha cargado
    document.addEventListener('DOMContentLoaded', () => {
        const contactForm = document.getElementById('contactForm');
        const messageInput = document.getElementById('message');
        const charCount = document.getElementById('charCount');

        // Contador de caracteres
        messageInput.addEventListener('input', () => {
            const currentLength = messageInput.value.length;
            charCount.textContent = `${currentLength}/250`;
            console.log(`Caracteres en el mensaje: ${currentLength}`);
        });

        // Envío del formulario
        contactForm.addEventListener('submit', function (event) {
            event.preventDefault();

            console.log('Formulario enviado. Recogiendo datos...');
            // Recoger los datos del formulario
            const formData = {
                nombresApellidos: String(document.getElementById('name').value),
                correoElectronico: String(document.getElementById('email').value),
                telefonoCelular: String(document.getElementById('phone').value),
                nombreEmpresa: String(document.getElementById('company').value),
                nitEmpresa: String(document.getElementById('nit').value),
                ciudadRegional: String(document.getElementById('city').value),
                datosInteres: String(Array.from(document.querySelectorAll('input[type="checkbox"]:checked')).map(cb => cb.value)),
                mensaje: String(document.getElementById('message').value)
            };

            console.log('Datos del formulario recogidos:', formData);

            // Llamada para obtener el token de acceso
            getAccessToken().then(token => {
                console.log('Token recibido, creando solicitud SOAP...');
                const soapRequest = createSOAPRequest(formData, token);
                console.log('Solicitud SOAP creada:', soapRequest);

                // Enviar la solicitud SOAP
                return fetch('https://usa258s.sfdc-yfeipo.salesforce.com/services/Soap/class/WSNegociosCorporativos', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'text/xml',
                        'SOAPAction': 'recibirNegociosCorporativos'
                    },
                    body: soapRequest
                });
            })
            .then(response => {
                console.log('Respuesta de la solicitud SOAP:', response);
                if (!response.ok) {
                    throw new Error(`Error en la solicitud SOAP: ${response.statusText}`);
                }
                return response.text();
            })
            .then(data => {
                console.log('Éxito:', data);
                // Manejo de la respuesta exitosa
            })
            .catch((error) => {
                console.error('Error en la solicitud SOAP:', error.message || error);
            });
        });
    });
