

<article class="site-page" data-template-page >
  <header class="page-masthead">
    <h1 class="container">
      {{ page.title }}
    </h1>
  </header>

  {% if page.content != blank %}
    <div class="page-content rte">
      {{ page.content }}
    </div>
  {% endif %}
  
  
  <div class="container headerOffset contact-center">
	<!--<header id="page-title">
		<div class="container">
			<h1>Contact Center</h1>
		</div>
	</header>
	-->
	<section id="contact" class="container">
		<div class="row">
			<div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
				<div class="text-center" style="margin-bottom:20px;">
					<p>	
						<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_callcenter.png' | asset_url }}"/>
					</p>
					<h4 class="title">Call Center</h4>
				</div>
				<div class="row">
					<div class="col-md-5 col-xs-5 text-right">Santa Cruz</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterSantaCruz}}</div>
					<div class="col-md-5 col-xs-5 text-right">La Paz</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterLaPaz}}</div>
					<div class="col-md-5 col-xs-5 text-right">Cochabamba</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterCochabamba}}</div>
					<div class="col-md-5 col-xs-5 text-right">Oruro</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterOruro}}</div>
					<div class="col-md-5 col-xs-5 text-right">Chuquisaca</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterChuquisaca}}</div>
					<div class="col-md-5 col-xs-5 text-right">Tarija</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.callCenterTarija}}</div>
				</div>
				<p>&nbsp;</p>
			</div>
			<div class="col-lg-3 col-md-6 col-sm-6 col-xs-12">
			 	<div class="text-center" style="margin-bottom:20px;">
                    <p>	
                    	<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_delivey.png' | asset_url }}"/>
                    </p>
                    <h4 class="title">Delivery Farmam&oacute;vil</h4>
			 	</div>
				<div class="row">
					<div class="col-md-5 col-xs-5 text-right">Santa Cruz</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilSantaCruz}}</div>
					<div class="col-md-5 col-xs-5 text-right">La Paz</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilLaPaz}}</div>
					<div class="col-md-5 col-xs-5 text-right">Cochabamba</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilCochabamba}}</div>
					<div class="col-md-5 col-xs-5 text-right">Oruro</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilOruro}}</div>
					<div class="col-md-5 col-xs-5 text-right">Chuquisaca</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilChuquisaca}}</div>
					<div class="col-md-5 col-xs-5 text-right">Tarija</div>
					<div class="col-md-6 col-xs-6 color-phone">{{settings.deliveryFarmamovilTarija}}</div>
				</div>
		 		<p>&nbsp;</p>
            </div>
            <div class="clearfix hidden-lg"></div>
			<div class="col-lg-2 col-md-6 col-sm-6 col-xs-12">
				<div class="text-center" style="margin-bottom:20px;">	
					<p>	
						<a href="{{settings.contactEmail}}">
							<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_mail.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="{{settings.contactEmail}}">Correo</a></h4>
				</div>
		 		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row" >
			 			<div class="wrapper">
							<ul>
								<li><a class="class-center-whapp-skype" href="{{settings.contactEmail}}">Cont&aacute;ctenos</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>
            </div>
			<div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
				<div class="text-center" style="margin-bottom:20px;">
					<p>
						<a href="{{settings.messengerFarmacorp}}" target="_blank" >
							<img class="img-responsive marginTB-20"width"100px" height="100px" src="{{'icon_messenger.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="{{settings.messengerFarmacorp}}" target="_blank" >Messenger</a></h4>
				</div>
		 		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row">
			 			<div class="wrapper">
							<ul>
								<li><a href="{{settings.messengerFarmacorp}}" target="_blank" class="class-center-whapp-skype">Enviar mensaje</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
            	<div class="text-center" style="margin-bottom:20px;">
					<p>
						<a href="{{settings.urlWhatsapp}}" target="_blank" >
							<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_whatsapp.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="https://api.whatsapp.com/send?phone=59175367559&text=Hola, mi consulta es" target="_blank" >Whatsapp</a></h4>
				</div>
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row">
			 			<div class="wrapper">
							<ul>
								<li><a href="{{settings.urlWhatsapp}}" target="_blank"  class="class-center-whapp-skype">{{settings.numeroWhatsapp}}</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>

            </div>
		</div>	
	</section>
</div>
  
</article>

<style>
ul, li{
    list-style:none;
    list-style-type:none;
}
</style>
