

<article class="site-page" data-template-page >
  <header class="page-masthead">
    <h1 class="container">
      {{ page.title }}
    </h1>
  </header>

  {% if page.content != blank %}
    <div class="page-content rte">
      {{ page.content }}
    </div>
  {% endif %}
  
  
  <div class="container headerOffset contact-center">
	<!--<header id="page-title">
		<div class="container">
			<h1>Contact Center</h1>
		</div>
	</header>
	-->
	<section id="contact" class="container">
		<div class="row">
          <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" ></div>
            <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
            	<div class="text-center" style="margin-bottom:20px;">
					<p>	
						<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_callcenter.png' | asset_url }}"/>
					</p>
                   <h4 class="title"><a href="{{settings.messengerFarmacorp}}" target="_blank" >Call Center y Delivery</a></h4>
				</div>
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row">
			 			<div class="wrapper">
							<ul>
								<li>{{settings.callCenterSantaCruz}}</li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>

            </div>
			<div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
				<div class="text-center" style="margin-bottom:20px;">	
					<p>	
						<a href="{{settings.contactEmail}}">
							<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_mail.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="{{settings.contactEmail}}">Correo</a></h4>
				</div>
		 		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row" >
			 			<div class="wrapper">
							<ul>
								<li><a class="class-center-whapp-skype" href="{{settings.contactEmail}}">Cont&aacute;ctenos</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>
            </div>
			<div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
				<div class="text-center" style="margin-bottom:20px;">
					<p>
						<a href="{{settings.messengerFarmacorp}}" target="_blank" >
							<img class="img-responsive marginTB-20"width"100px" height="100px" src="{{'icon_messenger.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="{{settings.messengerFarmacorp}}" target="_blank" >Messenger</a></h4>
				</div>
		 		<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row">
			 			<div class="wrapper">
							<ul>
								<li><a href="{{settings.messengerFarmacorp}}" target="_blank" class="class-center-whapp-skype">Enviar mensaje</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>
            </div>
            <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" >
            	<div class="text-center" style="margin-bottom:20px;">
					<p>
						<a href="{{settings.urlWhatsapp}}" target="_blank" >
							<img class="img-responsive marginTB-20" width"100px" height="100px" src="{{'icon_whatsapp.png' | asset_url }}"/>
						</a>
					</p>
					<h4 class="title"><a href="{{settings.urlWhatsapp}}" target="_blank" >Whatsapp</a></h4>
				</div>
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-center">
					<div class="row">
			 			<div class="wrapper">
							<ul>
								<li><a href="{{settings.urlWhatsapp}}" target="_blank"  class="class-center-whapp-skype">{{settings.numeroWhatsapp}}</a></li>
							</ul>
						  	<br />
						</div><!-- .wrapper -->
			 		</div>
			 	</div>

            </div>
           <div class="col-lg-2 col-md-6 col-sm-6 col-xs-12" ></div>
		</div>	
	</section>
</div>
  
</article>

<style>
ul, li{
    list-style:none;
    list-style-type:none;
}
</style>
