   function associateFarmaclubMethod(){
      
      if(jquery('#nextBtn').html() === `{{ 'farmacorp.vinculacion.finalizar' | t}}`){
          jquery('#nextBtn').html(`<i class="fa fa-spinner fa-spin"></i>&nbsp;&nbsp; Vinculando`);    
