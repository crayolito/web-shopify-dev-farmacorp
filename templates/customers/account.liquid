  function loadOrders(pageInfo, next){
      
      let id = `{{customer.id}}`;
      
      if(id)
      {

           var orders =  jquery('.orders');
          orders.empty();
          orders.append(`<i class="fa fa-refresh fa-spin"></i>  Cargando ordenes...`);
         
          getOrdenes(id, 5, 'any', 'any', 'any',next?pageInfo:'', !next?pageInfo:'')
          .then(function(response) { 
      
               orders.empty();

              if(response.data.length  > 0){

                localStorage["orders"] = JSON.stringify(response.data);

                var filas = '';
                response.data.forEach(element => {

                   var tracking = '';
                   if (element.fulfillmentStatus == '{{settings.estado_tracking }}')                 
                   {
                         tracking = `<a  href="https://farmacorp.com/pages/detalle-de-orden/"  onclick="showOrder('${element.id}');"> {{ 'farmacorp.tracking.link_ver_on_line' |t }}</a>`
                   }                   
                   var orderDetail = `<a  href="https://farmacorp.com/pages/order-detail/${element.id}" >${element.name}, ${element.createDate.split('T')[0]}</a>`;
                   var fulfillmentStatus = element.fulfillmentStatusDesc;
                   if(element.financialStatus=='pending' &&  element.formaPago == 'Pago QR')
                   {
                      fulfillmentStatus = `<a  href="${element?.orderPayment?.orderStatusUrl}" style="color: red;text-decoration: underline;"> Pago pendiente</a>`;
                   }
                  
                   filas +=`<tr>
                              <td class="account-order-number">
                                  ${orderDetail}
                              </td>

                              <td class="fulfillment-status--${ element.fulfillmentStatus }">
                                ${fulfillmentStatus}
                              </td>
                              <td class="payment-status--${ element.financialStatus }">
                                ${tracking}
                              </td>
                              <td class="account-order-list-total">
                                ${element.totalPrice}
                              </td>
                          </tr>`;
                });


                var disabledBack = '';
                var disabledNext = '';
                if(response.prevPageInfo=='')
                {
                  disabledBack='disabled';
                }

                if(response.nextPageInfo=='')
                {
                  disabledNext='disabled';
                }
                
               orders.append(`
					
                    <table class="account-table account-order-list">
                      <thead>
                        <tr>
                          <th>{{ 'customers.account.order' | t }}</th>
                          <th>{{ 'customers.account.fulfillment' | t }}</th>
                          <th>{{ 'farmacorp.tracking.titulo' |t }}</th>
                          <th>{{ 'customers.account.total' | t }}</th>
                        </tr>
                      </thead>
                        ${filas}	
                      <tbody>

                      </tbody>
                    </table>
                    <br />
                   <nav aria-label="Page navigation example">
                        <ul class="pagination justify-content-end">
                          <li class="page-item ${disabledBack }" >
                            <a class="page-link" onclick="loadOrders('${response.prevPageInfo }', false);">Anterior</a>
                          </li>
                          <li class="page-item ${disabledNext }" >
                            <a class="page-link" onclick="loadOrders('${response.nextPageInfo}', true);">Siguiente</a>
                          </li>
                        </ul>
                      </nav>

                `);
              }else{
               orders.append(`<p class="empty">{{ 'customers.account.empty' | t }}</p>`);
              } 
          }).catch(function(err) {  
           console.log(err);
          });
      }
  	}
  
