{% assign pagoEstado = 'shopify.notices.order.financial_status.paid' | t %}
{% assign pedidoEstado = 'shopify.notices.order.fulfillment_status.fulfilled' | t %}
{% assign estados = shop.metafields.global.estados %}

<!-- Modals -->
<div class="account-page account-page-details">
  <div class="account-page-content">
    <div id="editCustomer" class="modalCustomer" data-template-account>
      <div class="modalCustomer-content">
        <div class="modalCustomer-header">
          <h3>{{ 'customers.account.title' | t }}</h3>
        </div>
        <form onsubmit="updateInfo();">
          <div id="updateCustomer" class="modalCustomer-body">
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="customer_nombre_edit"
                type="text"
                required
                value="{{ customer.first_name }}"
              >

              <label class="form-field-title" for="customer_nombre">
                {{ 'customers.forms.first_name' | t }}
              </label>
            </div>
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="customer_apellido_edit"
                type="text"
                required
                value="{{ customer.last_name }}"
              >

              <label class="form-field-title" for="customer_apellido">
                {{ 'customers.forms.last_name' | t }}
              </label>
            </div>
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="customer_telefono_edit"
                type="phone"
                required
                minlength="8"
                maxlength="8"
                
                value="{% if customer.metafields.farmacorp.telefono %}{{ customer.metafields.farmacorp.telefono| replace: "+591", "" }}{% else %}{{ customer.phone | replace: "+591", ""}}{% endif %}"
              >

              <label class="form-field-title" for="customer_telefono">
                {{ 'customers.forms.phone' | t }}
              </label>
            </div>
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="customer_correo_edit"
                type="text"
                disabled
                value="{% if customer.metafields.farmacorp.emailAlterno %}{{ customer.metafields.farmacorp.emailAlterno }}{% else %}{{ customer.email }}{% endif %}"
              >

              <label class="form-field-title" for="customer_correo">
                {{ 'customers.forms.email' | t }}
              </label>
            </div>
          </div>
          <div class="modalCustomer-footer">
            <button
              id="cancelUpdateCustomer"
              type="button"
              class="btn btn-light"
              onclick="CloseModalCustomer('editCustomer');"
            >
              Cancelar
            </button>
            <button id="executeUpdateCustomer" type="submit" class="btn btn-light">Actualizar</button>
          </div>
        </form>
      </div>
    </div>

    <div id="passwordCustomer" class="modalCustomer">
      <div class="modalCustomer-content">
        <div class="modalCustomer-header">
          <h3>{{ 'farmacorp.perfil.cambiarContrasena' | t }}</h3>
        </div>

        <form onsubmit="changePasswordMethod();">
          <div id="changePassword" class="modalCustomer-body">
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="contrasenaActual"
                type="password"
                minlength="6"
                required
              >

              <label class="form-field-title" for="customer_nombre">
                {{ 'farmacorp.perfil.contrasenaActual' | t }}
              </label>
            </div>
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="contrasenaNueva"
                type="password"
                minlength="6"
                required
              >

              <label class="form-field-title" for="customer_apellido">
                {{ 'farmacorp.perfil.contrasenaNueva' | t }}
              </label>
            </div>
            <div class="form-field">
              <input
                class="form-field-input form-field-text"
                id="contrasenaNuevaRepetir"
                type="password"
                required
                minlength="6"
              >

              <label class="form-field-title" for="customer_telefono">
                {{ 'farmacorp.perfil.contrasenaNuevaRepetir' | t }}
              </label>
            </div>
          </div>
          <div class="modalCustomer-footer">
            <button
              id="cancelChangePass"
              type="button"
              class="btn btn-light"
              onclick="CloseModalCustomer('passwordCustomer');"
            >
              Cancelar
            </button>
            <button id="executeChangePass" type="submit" class="btn btn-ligh">Ok</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
<!-- End Modals -->

