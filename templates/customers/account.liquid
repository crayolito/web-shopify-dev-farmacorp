    var currentTab = 0;
    showTab(currentTab); 

    function showTab(n) {
      var x = document.getElementsByClassName("tab");
      x[n].style.display = "block";
      if (n === 0) {
        document.getElementById("prevBtn").style.display = "none";
      } else {
        document.getElementById("prevBtn").style.display = "inline";
      }
      if (n === (x.length - 1)) {
        
        document.getElementById("customer_nombre_final").value = document.getElementById("customer_nombre").value;
        document.getElementById("customer_apellido_final").value = document.getElementById("customer_apellido").value;
        document.getElementById("customer_telefono_final").value = document.getElementById("customer_telefono").value;
        document.getElementById("customer_correo_final").value = document.getElementById("customer_correo").value;
        document.getElementById("customer_ci_final").value = document.getElementById("customer_ci").value;
        document.getElementById("customer_fecha_final").value = document.getElementById("customer_fecha").value;
        
        document.getElementById("nextBtn").innerHTML = "{{ 'farmacorp.vinculacion.finalizar' | t}}";
        document.getElementById("prevBtn").style.display = "none";
      } else {
        document.getElementById("nextBtn").innerHTML = "{{ 'farmacorp.vinculacion.siguiente' | t}}";
      }
      fixStepIndicator(n)
    }

    function nextPrev(n) {
      var x = document.getElementsByClassName("tab");
      if (n == 1 && !validateForm()){
        return false;
      }
      x[currentTab].style.display = "none";
      currentTab = currentTab + n;
      if (currentTab >= x.length) {
        //document.getElementById("regForm").submit();
        showTab(currentTab - 1);
        associateFarmaclubMethod();
        
        
        return false;
      }else
      {
      	showTab(currentTab);
      }
    }

    function validateForm() {
      var x, y, i, valid = true;
      x = document.getElementsByClassName("tab");
      y = x[currentTab].getElementsByTagName("input");
      for (i = 0; i < y.length; i++) {
        if (y[i].value == "") {
          y[i].className += " invalid";
          valid = false;
        }
      }
      if (valid) {
        document.getElementsByClassName("step")[currentTab].className += " finish";
      }
      return valid;
    }

    function fixStepIndicator(n) {
      var i, x = document.getElementsByClassName("step");
      for (i = 0; i < x.length; i++) {
        x[i].className = x[i].className.replace(" active", "");
      }
      x[n].className += " active";
    }
  
  
   function associateFarmaclubMethodNew(){
       
       var respuesta = {};
       
        Swal.fire({
                /*title: `{{ 'farmacorp.vinculacion.titulo' | t}}`,*/
                /*text: `{{ 'farmacorp.vinculacion.cuenta_subtitulo' | t}}`,*/
          		text: `{{ 'farmacorp.vinculacion.titulo' | t}}`,
                imageUrl: 'https://cdn.shopify.com/s/files/1/0368/1363/5716/files/FARMACLUB-320x120.png?v=1636551373',
                imageWidth: 300,
                imageHeight: 100,
                input: `text`,
                inputAttributes: {
                  autocapitalize: `off`,
          		  placeholder:`{{ 'farmacorp.vinculacion.documento_identidad' | t}}`,
                  required: true
                },
                showCancelButton: true,
                confirmButtonText: `{{ 'farmacorp.vinculacion.siguiente' | t}}`,
                confirmButtonColor: '#fc2116',
                cancelButtonText: `{{ 'farmacorp.vinculacion.anterior' | t}}`,
                showCloseButton: true,
                showLoaderOnConfirm: true,
                footer: `<input type="checkbox"  class="footerAlert" id="chbNo" oninput="noVolverAPreguntar(this)"> <label  class="footerAlert2" for="chbNo">No volver a preguntar.</label>`,
 				preConfirm: (documento) => {
			
                  
          			 return associateFarmaclubV2(
                       		`{{ customer.first_name }}`,
                            `{{ customer.last_name }}`, 
                            `{{ customer.email }}`, 
                            `{{ customer.phone }}`,
                            ``,//fecha de nacimiento 
                            documento, 
                            `{{ customer.id | json }}`,
                            `{{ customer.email }}`)
                    .then(function(response) { 
                       
                     
                          if(response.errors.length>0)
                          {
                                let e1 = JSON.parse(response.errors[0].message);
                            	if(e1.length>= 1){
                                	Swal.showValidationMessage(e1[0].Message);
                                }else
                                {
                            		Swal.showValidationMessage(response.errors[0].message);
                                }
                          }else
                          {
                            	respuesta = {
                                  nombre : response.data.nombres, 
                                  apellido : response.data.apellidos, 
                                  correo : response.data.email, 
                                  telefono:response.data.celular,
                                  cedula:response.data.cedula ,
                                  idFarmacorp:response.data.idFarmacorp 
                                };
                            	return respuesta;
                          }  
                    })
                    .catch(function(err) {
                          Swal.showValidationMessage( err.responseJSON.message);
                    });

  				},
  				allowOutsideClick: () => !Swal.isLoading()
                }).then((result) => {
                  if (result.isConfirmed) {
                   		 Swal.fire({
                              title: '<strong>Valida tu identidad</strong>',
                              icon: 'question',
                              html:
                           		`
								<div class="align-left">	
									<b>Nombre : </b>${ respuesta.nombre===null?``:respuesta.nombre}<br/>
									<b>Apellido : </b>${ respuesta.apellido===null?``:respuesta.apellido}<br/>
									<b>Correo : </b>${ respuesta.correo===null || respuesta.correo=== ``?`{{ customer.email }}`:respuesta.correo}<br/>
									<b>Teléfono : </b>${ respuesta.telefono===null || respuesta.telefono=== ``?`{{ customer.phone }}`:respuesta.telefono}<br/>
								</div><br/>
								¿Estos datos te pertenecen?
								`,
                              showCloseButton: true,
                              showCancelButton: true,
                              focusConfirm: true,
                              confirmButtonText:'Si, soy yo',
                              cancelButtonText:'Cancelar',
                              confirmButtonColor: '#fc2116',
                              showLoaderOnConfirm: true,
                            }).then((result) => {
                                if (result.isConfirmed) {
                                  
                                  
                                     return attachFarmaclub(
                                            respuesta.cedula,
                                            respuesta.idFarmacorp)
                                    .then(function(response) { 

                                          if(response.errors.length>0)
                                          {
                                              Swal.fire('Ocurrio un error!', response.errors[0].message, 'error');
                                            
                                          }else
                                          {
                                                Swal.fire('Cuenta vinculada de manera exitosa!', '', 'success')
                                                .then((result) => {
                  									if (result.isConfirmed) {
                                                		location.reload();
                                                    }
                                                });
                                          }  
                                    })
                                    .catch(function(err) {
                                          Swal.showValidationMessage( err.responseJSON.message);
                                    });
                                  
                                  
                                  
                                 
                                } else
                                  Swal.fire('No se vinculo tu cuenta', '', 'info')
                                }
                              )
                  }
                });
 
    
    }










